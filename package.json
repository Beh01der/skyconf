{
  "name": "skyconf",
  "version": "1.0.0",
  "packageManager": "yarn@3.6.0",
  "description": "Simple REST-full FS-backed document storage",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "url": "https://github.com/Beh01der/skyconf.git"
  },
  "scripts": {
    "dev": "nodemon",
    "bump": "yarn version patch && git push --tags && git push --all",
    "bundle": "rm -rf ./dist && esbuild --bundle src/index.ts --platform=node --target=node18 --minify --keep-names --sourcemap --outdir=dist",
    "format": "dprint fmt ./src/**/*",
    "prepare": "yarn bundle"
  },
  "keywords": [
    "REST",
    "JSON",
    "db",
    "storage",
    "config",
    "persistence",
    "JSONPath",
    "json-patch"
  ],
  "author": "Andrey Chausenko",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.17",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "esbuild": "^0.18.17",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "fast-json-patch": "^3.1.1",
    "jsonpath-plus": "^7.2.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
